<div id="<%= dom_id review %>">
  <p>
    <strong>Review text:</strong>
    <%= review.review_text %>
  </p>

  <p>
    <strong>Rating:</strong>
    <%= review.rating %>
  </p>

  <p>
    <strong>Review date:</strong>
    <%= review.review_date %>
  </p>

  <p>
    <strong>User:</strong>
    <%= review.user_id %>
  </p>

  <p>
    <strong>Tour:</strong>
    <%= review.tour_id %>
  </p>

</div>
